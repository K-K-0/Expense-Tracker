
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  name String
  email String @unique
  password String
  expenses Expense[]
}

model Expense {
  id        String @id @default(uuid())
  title     String
  amount    Int
  category  String
  date      DateTime @default(now())
  user      User  @relation(fields: [userId], references: [id])
  userId    Int
}
